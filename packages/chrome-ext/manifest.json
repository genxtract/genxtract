{
  "name": "genxtract",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Development environment for genxtract",
  "homepage_url": "https://github.com/genxtract/genxtract",
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "genxtract"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
